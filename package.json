{
  "name": "fluorite",
  "version": "1.0.2",
  "description": "Lightweight ORM based on Knex.js query builder.",
  "main": "dist/index.js",
  "repository": "https://github.com/pldin601/Fluorite.js",
  "engines": {
    "node": "^8"
  },
  "scripts": {
    "test": "jest --verbose",
    "lint": "eslint src/",
    "cover": "jest --coverage && (cat ./coverage/lcov.info | coveralls)",
    "build": "NODE_ENV=production babel src --out-dir dist --source-maps inline",
    "prepublish": "npm run build"
  },
  "author": "Roman Lakhtadyr <roman.lakhtadyr@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^20.0.8",
    "@types/knex": "0.0.57",
    "@types/lodash": "^4.14.72",
    "babel-cli": "*",
    "babel-eslint": "*",
    "babel-jest": "^20",
    "babel-plugin-transform-builtin-extend": "*",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "*",
    "babel-preset-stage-0": "*",
    "coveralls": "*",
    "eslint": "*",
    "eslint-config-airbnb-base": "*",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "*",
    "eslint-plugin-jest": "^20",
    "jest": "^20",
    "sqlite3": "*",
    "ts-jest": "^21.0.1",
    "typescript": "^2.5.3"
  },
  "dependencies": {
    "continuation-local-storage": "^3.2.0",
    "knex": "*",
    "lodash": "*",
    "stream-to-async-iterator": "^0.2.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^.+\\.jsx?$": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
